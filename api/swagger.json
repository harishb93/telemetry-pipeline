{
    "swagger": "2.0",
    "info": {
        "description": "API Gateway for GPU Telemetry Pipeline",
        "title": "Telemetry API Gateway",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "email": "support@example.com"
        },
